<h1>Users::Rooms#index</h1>
ルーム検索<br>
<%= form_with url: search_room_path, method: :get, local:true do |f| %>
  <%= f.label :search_room %>
  <%= f.number_field :search_room, min: 1, max: @room_all.count %>
  <%= f.submit "検索する" %>
<% end %>

ルーム一覧<br>
<% @rooms.each do |room| %>
  <%= room.id %>
  <%= room.name %>
  <%= room.user.name %><%#ルームの作成者%>
  <%= room.room_users.count %><%#ルームの参加人数%>
  <%= room.created_at %>
  <%= link_to "ルームに入る", room_path(room.id) %>
  <br>
<% end %>